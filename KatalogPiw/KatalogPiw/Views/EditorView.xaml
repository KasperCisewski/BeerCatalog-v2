<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KatalogPiw.Views.EditorView">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="ObjectName" Text="{Binding Nazwa}" HorizontalOptions="Center"/>
            <Entry x:Name="NewObjectName" Text="Nowa nazwa" VerticalOptions="Center"/>
            <Button x:Name="Save" Text="Zmien" Clicked="OnSaveClick" VerticalOptions="FillAndExpand" WidthRequest="30"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>